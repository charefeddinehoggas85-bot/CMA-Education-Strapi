    "debouches": {
      "type": "json"
    },
    "duree": {
      "type": "string"
    },
    "volumeHoraire": {
      "type": "string"
    },
    "repartition": {
      "type": "string"
    },
    "rythme": {
      "type": "string"
    },
    "modalite": {
      "type": "string"
    },
    "typeContrat": {
      "type": "string"
    },
    "effectif": {
      "type": "string"

      "allowedTypes": [
        "images"
      ]
    },
    "brochure": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "ordre": {
      "type": "integer",
      "default": 1
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "seoTitle": {
      "type": "string"
    },
    "seoDescription": {
      "type": "text"
    },
    "seoKeywords": {
      "type": "json"
    },
